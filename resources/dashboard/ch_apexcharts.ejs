<!DOCTYPE html>
<html lang="en">

<head>
    <% include resources/dashboard/partials/meta.ejs %>

    <title><%= htmlWebpackPlugin.options.title %></title>

    <% include resources/dashboard/partials/styles.ejs %>
    <link rel="stylesheet" href="assets/vendors/apexcharts/apexcharts.css">
</head>

<body class="theme-dark">
    <div class="grid-wrapper sidebar-bg bg1">

        <!-- BOF HEADER -->
        <% include resources/dashboard/partials/header.ejs %>
        <!-- EOF HEADER -->

        <!-- BOF ASIDE-LEFT -->
        <% include resources/dashboard/partials/asideleft.ejs %>
        <!-- EOF ASIDE-LEFT -->

        <!-- BOF MAIN -->
        <div class="main">

            <!-- BOF Breadcrumb -->
            <div class="row">
                <div class="col">
