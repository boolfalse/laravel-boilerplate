                </div>
                <div class="col-md-6">
                    <div class="card mb-3">
                        <div class="card-body" id="chart7"></div>
                    </div>
                </div>
            </div>
            <!-- EOF MAIN-BODY -->

        </div>
        <!-- EOF MAIN -->

        <!-- BOF FOOTER -->
        <% include resources/dashboard/partials/footer.ejs %>
        <!-- EOF FOOTER -->

        <!-- BOF ASIDE-RIGHT -->
        <% include resources/dashboard/partials/asideright.ejs %>
        <!-- EOF ASIDE-RIGHT -->

        <div id="overlay"></div>

    </div> <!-- END WRAPPER -->

    <% include resources/dashboard/partials/scripts.ejs %>
    <script src="assets/vendors/apexcharts/apexcharts.js"></script>
    <script src="assets/scripts/pages/ch_apexcharts.js"></script>
